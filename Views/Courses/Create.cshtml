@model Course
@{
    ViewData["Title"] = "Create";
}

<h1></h1>
<form method="post" enctype="multipart/form-data">

    <div class="form-group">
        <label asp-for="@Model.Ad"></label>
        <input asp-for="@Model.Ad" class="form-control">
        <span asp-validation-for="@Model.Ad" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Qrup_sayi"></label>
        <input asp-for="@Model.Qrup_sayi" class="form-control">
        <span asp-validation-for="@Model.Qrup_sayi" class="text-danger"></span>
    </div>


    <button type="submit" class="btn btn-primary"><i class="mdi mdi-folder-plus"></i>Create</button>
    <a asp-controller="Courses" asp-action="Index" class="btn btn-success"><i class="mdi mdi-folder-upload"></i>Go Back</a>
</form>
@section Scripts{

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
}


